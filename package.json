    "license": "ISC",
    "dependencies": {
        "body-parser": "^1.19.1",
        "concurrently": "^7.0.0",
        "cross-env": "^7.0.3",
        "express": "^4.17.2",
        "mongodb": "^4.3.0",
        "mongoose": "^6.1.7",
        "nodemon": "^2.0.15"
    },
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-preset-env": "^1.7.0",
        "babel-preset-stage-0": "^6.24.1"
    }
}
